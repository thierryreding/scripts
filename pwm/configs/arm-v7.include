# for PWM_HIBVT
kconfig enable ARCH_HISI
# for PWM_IMX
kconfig enable ARCH_MXC
# for PWM_LP3943
kconfig enable MFD_LP3943
# for PWM_MESON
kconfig enable ARCH_MESON
# for PWM_MTK_DISP
kconfig enable ARCH_MEDIATEK
# for PWM_RCAR
kconfig enable ARCH_RENESAS
kconfig enable ARCH_R8A7778
kconfig enable ARCH_R8A7779
kconfig enable ARCH_R8A7790
kconfig enable ARCH_R8A7791
kconfig enable ARCH_R8A7793
kconfig enable ARCH_R8A7794
# for PWM_RENESAS_TPU
kconfig enable ARCH_RENESAS
# for PWM_ROCKCHIP
kconfig enable ARCH_ROCKCHIP
# for PWM_SAMSUNG
kconfig enable ARCH_EXYNOS
# for PWM_SPEAR
kconfig enable PLAT_SPEAR
# for PWM_STI
kconfig enable ARCH_STI
# for PWM_STMPE
kconfig enable MFD_STMPE
# for PWM_SUN4I
kconfig enable ARCH_SUNXI
# for PWM_TEGRA
kconfig enable ARCH_TEGRA
# for PWM_TIECAP and PWM_TIEHRPWM
kconfig enable ARCH_DAVINCI_DA8XX
kconfig enable ARCH_OMAP3
kconfig enable ARCH_OMAP4
# for PWM_TWL and PWM_TWL_LED
kconfig enable TWL4030_CORE
# for PWM_VT8500
kconfig enable ARCH_WM8850
# PWM drivers
kconfig enable PWM
kconfig enable PWM_AB8500
kconfig enable PWM_ATMEL
kconfig enable PWM_ATMEL_HLCDC_PWM
kconfig enable PWM_ATMEL_TCB
kconfig enable PWM_BCM_IPROC
kconfig enable PWM_BCM_KONA
kconfig enable PWM_BRCMSTB
kconfig enable PWM_BERLIN
kconfig enable PWM_CROS_EC
kconfig enable PWM_FSL_FTM
kconfig enable PWM_HIBVT
kconfig enable PWM_IMX
kconfig enable PWM_LP3943
kconfig enable PWM_MESON
kconfig enable PWM_MTK_DISP
kconfig enable PWM_PCA9685
kconfig enable PWM_RCAR
kconfig enable PWM_RENESAS_TPU
kconfig enable PWM_ROCKCHIP
kconfig enable PWM_SAMSUNG
kconfig enable PWM_SPEAR
kconfig enable PWM_STI
kconfig enable PWM_STMPE
kconfig enable PWM_SUN4I
kconfig enable PWM_TEGRA
kconfig enable PWM_TIECAP
kconfig enable PWM_TIEHRPWM

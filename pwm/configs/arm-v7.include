kconfig enable AVR32
kconfig enable MFD_ATMEL_HLCDC
kconfig enable ATMEL_TCLIB
# for PWM_BCM_KONA and PWM_BRCMSTB
kconfig enable ARCH_BCM
# for PWM_BCM_KONA
kconfig enable ARCH_BCM_281XX
# for PWM_BRCMSTB
kconfig enable ARCH_BRCMSTB
# for PWM_BCM_IPROC
kconfig enable ARCH_BCM_CYGNUS
kconfig enable ARCH_BCM_NSP
kconfig enable ARCH_BCM_5301X
# for PWM_BERLIN
kconfig enable ARCH_BERLIN
# for PWM_CROS_EC

kconfig architecture arm
kconfig allnoconfig
# basic configuration
kconfig enable DEBUG_FS
kconfig enable MMU
kconfig enable SYSFS
# bus configuration
kconfig enable I2C
# architecture configuration
kconfig disable ARM_SINGLE_ARMV7M
kconfig enable ARCH_MULTIPLATFORM
kconfig enable ARCH_MULTI_V7
# for PM_AB8500
kconfig enable ARCH_U300
kconfig enable ARCH_U8500
kconfig enable UX500_SOC_DB8500
kconfig enable ABX500_CORE
kconfig enable MFD_DB8500_PRCMU
kconfig enable AB8500_CORE
# for PWM_ATMEL, PWM_ATMEL_HLCDC_PWM and PWM_ATMEL_TCB
